{
    "name": "martin1982/live-broadcast-demo",
    "type": "project",
    "license": "MIT",
    "description": "Demo for the Live Broadcast Bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4||>=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.6",
        "martin1982/live-broadcast-easyadmin-bundle": "dev-master",
        "martin1982/live-broadcast-sonata-admin-bundle": "dev-master",
        "sensio/framework-extra-bundle": "^4.4|^5.4|^6.0",
        "symfony/console": "^4.4|^5.4|^6.0",
        "symfony/dotenv": "^4.4|^5.4|^6.0",
        "symfony/flex": "^1.6|^2.1",
        "symfony/framework-bundle": "^4.4|^5.4|^6.0",
        "symfony/monolog-bundle": "^2.0|^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/mailer": "^4.4|^5.4|^6.0",
        "symfony/templating": "^4.4|^5.4|^6.0",
        "symfony/yaml": "^4.4|^5.4|^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "^1.2",
        "symfony/phpunit-bridge": "^4.4|^5.4|^6.0",
        "symfony/stopwatch": "^4.4|^5.4|^6.0",
        "symfony/twig-bundle": "^4.4|^5.4|^6.0",
        "symfony/web-profiler-bundle": "^4.4|^5.4|^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "docker": false
        }
    }
}
