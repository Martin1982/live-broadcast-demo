{
    "name": "martin1982/live-broadcast-demo",
    "type": "project",
    "license": "MIT",
    "description": "Demo for the Live Broadcast Bundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2||>=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "incenteev/composer-parameter-handler": "^2.1",
        "sensio/framework-extra-bundle": "^5.1||^6.0",
        "martin1982/live-broadcast-sonata-admin-bundle": "^2.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.15",
        "symfony/console": "^4.4||^5.0",
        "symfony/dotenv": "^4.4||^5.0",
        "symfony/flex": "^1.6",
        "symfony/framework-bundle": "^4.4||^5.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/polyfill-apcu": "^1.15",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/templating": "^4.4||^5.0",
        "symfony/yaml": "^4.4||^5.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "^4.4||^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
